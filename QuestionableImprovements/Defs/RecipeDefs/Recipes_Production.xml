<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Make_ComponentIndustrialternive</defName>
    <label>scavenge a component</label>
    <description>Makes a component. Since the machining table doesn't have the adequate tools to create components, this recipe is inefficient, requiring nearly double the amount of steel.</description>
    <jobString>Scavenging a component.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>6000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ComponentIndustrial>1</ComponentIndustrial>
    </products>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <researchPrerequisite>Machining</researchPrerequisite>
  </RecipeDef>

</Defs>
