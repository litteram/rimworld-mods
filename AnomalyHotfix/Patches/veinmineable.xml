<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationAdd">
    <success>Always</success>
    <xpath>Defs/ThingDef[@ParentName="RockBase"]/building[
      isResourceRock="true"
      and not(veinMineable)
    ]</xpath>
    <value>
      <veinMineable>true</veinMineable>
    </value>
  </Operation>
</Patch>
